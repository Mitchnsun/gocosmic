{
  "name": "gocosmic",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --cache --cache-location .eslintcache --cache-strategy content --max-warnings 0",
    "lint:timing": "TIMING=1 eslint . --cache --cache-location .eslintcache --cache-strategy content --max-warnings 0",
    "check-types": "tsc --project tsconfig.check.json",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:snapshot": " vitest run -u",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "npx husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --cache-location .eslintcache --cache-strategy content --max-warnings 0",
      "prettier --write"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/drei": "^10.7.2",
    "@react-three/fiber": "^9.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "next": "^15.5.0",
    "next-intl": "^4.3.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^2.6.0",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.0",
    "@next/eslint-plugin-next": "^15.3.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.10.3"
}
